#include <stdio.h>
#include <stdlib.h>
#include "lists.h"
/**
 * print_listint - prints all the elements of the list
 * @h: a pointer to first node of the list
 * Return: number of nodes in the list
 */
size_t print_listint(const listint_t *h)
{
  listint_t *ptr;
  size_t count = 0;
  if (*h == NULL)
    return;
  ptr = h;
  while (ptr->next != NULL)
    {
      printf("%d", ptr->n);
      count++;
    }
  return (count);
}
