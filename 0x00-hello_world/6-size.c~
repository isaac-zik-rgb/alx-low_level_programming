#include <stdio.h>
/**
 * main - Entry point
 *
 * Return: 0 (Success)
 */
int main(void)
{
printf("Size of a char: %d", sizeof(char));
printf("byte(s)"\n);
printf("Size of an int: %d", sizeof(int));
printf("byte(s)\n");
printf("Size of a long int: %d", sizeof(int));
printf("Size of a long long int: %d", sizeof(long long int));
prinf("byte(s)\n");
printf("Size of a float: %d", sizeof(float));
printf("byte(s)");
return (0);
}
